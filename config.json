{
  "confluence": {
    "baseUrl": "https://<id_here>.atlassian.net//wiki",
    "apiToken": "<token_here>:https://id.atlassian.com/manage-profile/security/api-tokens",
    "username": "<email_here>"
  },
  "export": {
    "spaceKey": "<space_key_here>",
    "outputDir": "./output",
    "recursive": true,
    "includeAttachments": true,
    "concurrentRequests": 5,
    "format": {
      "includeFrontMatter": true,
      "preserveLinks": true
    }
  },
  "logging": {
    "level": "info",
    "file": "confluence-export.log"
  }
}
